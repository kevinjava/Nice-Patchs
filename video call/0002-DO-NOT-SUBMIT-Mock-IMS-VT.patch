diff --git a/ims/src/com/qualcomm/ims/vt/MockCommandHandler.java b/ims/src/com/qualcomm/ims/vt/MockCommandHandler.java
new file mode 100644
index 0000000..5b1196b
--- /dev/null
+++ b/ims/src/com/qualcomm/ims/vt/MockCommandHandler.java
@@ -0,0 +1,262 @@
+/*
+ * Copyright (C) 2015 Sony Mobile Communications Inc.
+ * All rights, including trade secret rights, reserved.
+ */
+
+package com.qualcomm.ims.vt;
+
+import android.content.BroadcastReceiver;
+import android.content.Context;
+import android.content.Intent;
+import android.content.IntentFilter;
+
+import org.codeaurora.ims.ImsCallSessionImpl;
+
+import java.util.Collections;
+import java.util.Set;
+import java.util.concurrent.ConcurrentHashMap;
+
+/**
+ * Handles the simulated commands for Mock IMS VT.
+ */
+final class MockCommandHandler {
+
+    // These values will be used for determining a size of the far-end images.
+    // The size will always be 4:3.
+    private static final int LONG_SIDE_PEER_IMAGES = ImsMediaConstants.DEFAULT_HEIGHT;
+    private static final int SHORT_SIDE_PEER_IMAGES = ImsMediaConstants.DEFAULT_WIDTH;
+
+    // Default value for generic usage.
+    private static final int DEFALUT = -100;
+
+    private Context mContext;
+    private BroadcastReceiver mReceiver;
+    private static MockCommandHandler sInstance;
+
+    private final Set<MockListener> mListeners = Collections.newSetFromMap(
+            new ConcurrentHashMap<MockListener, Boolean>(8, 0.9f, 1));
+    private final Set<MockConfigurationListener> mConfigurationListeners = Collections
+            .newSetFromMap(new ConcurrentHashMap<MockConfigurationListener, Boolean>(8, 0.9f, 1));
+
+    // Indicates the far-end device's orientation.
+    private int mPeerOrientation = DEFALUT;
+
+    // Indicates the player state.
+    private int mPlayerState = DEFALUT;
+
+    // Indicates the current video quality.
+    private int mVideoQuality = DEFALUT;
+
+    /**
+     * Listener interface to be used to notify the mock class of simulated event.
+     * This interface can be extended if we need a new simulation.
+     */
+    interface MockListener {
+        void onMockedPeerResolutionChanged(int width, int height);
+        void onMockedPlayerStateChanged(int state);
+        void onMockedVideoQualityEvent(int videoQuality);
+        void onMockedDataUsageChanged(int mediaId, long uplink, long downlink);
+    };
+
+    /**
+     * Listener interface to be used to notify the mock class of configuration changes.
+     */
+    interface MockConfigurationListener {
+        void onConfigurationApplied(boolean peerVideoEnabled, boolean peerCvoSupport,
+                long delayForVideoReception, long delayForDataUsageCallback);
+    };
+
+   /**
+    * Receives the simulated CVO mode via Broadcast that is sent by the ADB.
+    * On a terminal, we can send the Broadcast with entering a debug command such as below.
+    *
+    * $ adb shell am broadcast -a com.sonymobile.ims.vt.ACTION_PEER_ORIENTATION_CHANGED
+    *   --ei cvo_mode 1
+    *
+    *   @see MockIntents
+    */
+    private static final class MockCommandReceiver extends BroadcastReceiver {
+
+        private final MockCommandHandler mParent;
+
+        private MockCommandReceiver(MockCommandHandler parent) {
+            mParent = parent;
+        }
+
+        @Override
+        public void onReceive(Context context, Intent intent) {
+            final String action = intent.getAction();
+            if (MockIntents.ACTION_PEER_ORIENTATION_CHANGED.equals(action)) {
+                mParent.onPeerOrientationChanged(intent);
+            } else if (MockIntents.ACTION_PLAYER_STATE_CHANGED.equals(action)) {
+                mParent.onPlayerStateChanged(intent);
+            } else if (MockIntents.ACTION_VIDEO_QUALITY_CHANGED.equals(action)) {
+                mParent.onVideoQualityChanged(intent);
+            } else if (MockIntents.ACTION_DATA_USAGE_CHANGED.equals(action)) {
+                mParent.onDataUsageChanged(intent);
+            } else if (MockIntents.ACTION_CONFIG_APPLIED.equals(action)) {
+                mParent.onConfigurationApplied(intent);
+            }
+        }
+    }
+
+    // Should never be instantiated from the outer.
+    private MockCommandHandler() {
+    }
+
+    synchronized static MockCommandHandler getInstance() {
+        if (sInstance == null) {
+            sInstance = new MockCommandHandler();
+        }
+        return sInstance;
+    }
+
+    synchronized void init(Context context) {
+        if (context != null) {
+            mContext = context;
+            mReceiver = new MockCommandReceiver(this);
+            mContext.registerReceiver(mReceiver, createIntentFilter());
+        }
+    }
+
+    synchronized void addListener(MockListener listener) {
+        if (listener != null) {
+            mListeners.add(listener);
+        }
+    }
+
+    synchronized void removeListener(MockListener listener) {
+        if (listener != null) {
+            mListeners.remove(listener);
+        }
+    }
+
+    synchronized void addConfigurationListener(MockConfigurationListener listener) {
+        if (listener != null) {
+            mConfigurationListeners.add(listener);
+        }
+    }
+
+    synchronized void removeConfigurationListener(MockConfigurationListener listener) {
+        if (listener != null) {
+            mConfigurationListeners.remove(listener);
+        }
+    }
+
+    private IntentFilter createIntentFilter() {
+        // We can extend this filter if a new simulating needs to be added.
+        IntentFilter filter = new IntentFilter();
+        filter.addAction(MockIntents.ACTION_PEER_ORIENTATION_CHANGED);
+        filter.addAction(MockIntents.ACTION_PLAYER_STATE_CHANGED);
+        filter.addAction(MockIntents.ACTION_VIDEO_QUALITY_CHANGED);
+        filter.addAction(MockIntents.ACTION_DATA_USAGE_CHANGED);
+        filter.addAction(MockIntents.ACTION_CONFIG_APPLIED);
+        return filter;
+    }
+
+    /**
+     * Handles the change of the far-end device's orientation.
+     */
+    private void onPeerOrientationChanged(Intent intent) {
+        final int orientation = intent.getIntExtra(MockIntents.EXTRA_ORIENTATION, mPeerOrientation);
+        if (mPeerOrientation != orientation) {
+            int width;
+            int height;
+            switch (orientation) {
+                case ImsMediaConstants.PORTRAIT_MODE:
+                    width = SHORT_SIDE_PEER_IMAGES;
+                    height = LONG_SIDE_PEER_IMAGES;
+                    break;
+                case ImsMediaConstants.LANDSCAPE_MODE:
+                    width = LONG_SIDE_PEER_IMAGES;
+                    height = SHORT_SIDE_PEER_IMAGES;
+                    break;
+                default:
+                    return;
+            }
+            for (MockListener listener : mListeners) {
+                listener.onMockedPeerResolutionChanged(width, height);
+            }
+            mPeerOrientation = orientation;
+        }
+    }
+
+    /**
+     * Handles the change of the player state.
+     */
+    private void onPlayerStateChanged(Intent intent) {
+        final int state = intent.getIntExtra(MockIntents.EXTRA_PLAYER_STATE, mPlayerState);
+        if (mPlayerState != state) {
+            switch (state) {
+                case ImsMedia.PLAYER_STATE_STARTED:
+                case ImsMedia.PLAYER_STATE_STOPPED:
+                    // Only these are supported.
+                    break;
+                default:
+                    return;
+            }
+            for (MockListener listener : mListeners) {
+                listener.onMockedPlayerStateChanged(state);
+            }
+            mPlayerState = state;
+        }
+    }
+
+    /**
+     * Handles the change of the video quality.
+     */
+    private void onVideoQualityChanged(Intent intent) {
+        final int quality = intent.getIntExtra(MockIntents.EXTRA_VIDEO_QUALITY, mVideoQuality);
+        if (mVideoQuality != quality) {
+            switch (quality) {
+                case ImsMediaConstants.VIDEO_QUALITY_UNKNOWN:
+                case ImsMediaConstants.VIDEO_QUALITY_LOW:
+                case ImsMediaConstants.VIDEO_QUALITY_MEDIUM:
+                case ImsMediaConstants.VIDEO_QUALITY_HIGH:
+                    // Only these are supported.
+                    break;
+                default:
+                    return;
+            }
+            for (MockListener listener : mListeners) {
+                listener.onMockedVideoQualityEvent(quality);
+            }
+            mVideoQuality = quality;
+        }
+    }
+
+    /**
+     * Handles the change of the data usage.
+     */
+    private void onDataUsageChanged(Intent intent) {
+        final ImsCallSessionImpl activeSession = ImsVideoGlobals.getInstance()
+                .getActiveCallSession();
+        if (activeSession == null) {
+            return;
+        }
+        final int mediaId = activeSession.getMediaId();
+        final long uplink = intent.getLongExtra(MockIntents.EXTRA_UPLINK, 0);
+        final long downlink = intent.getLongExtra(MockIntents.EXTRA_DOWNLINK, 0);
+        for (MockListener listener : mListeners) {
+            listener.onMockedDataUsageChanged(mediaId, uplink, downlink);
+        }
+    }
+
+    /**
+     * Handles the change of the configurations.
+     */
+    private void onConfigurationApplied(Intent intent) {
+        final boolean peerVideoEnabled = intent.getBooleanExtra(
+                MockIntents.EXTRA_PEER_VIDEO_ENABLED, true);
+        final boolean peerCvoSupport = intent.getBooleanExtra(MockIntents.EXTRA_PEER_CVO_SUPPORT,
+                true);
+        final long delayForVideoReception = intent.getLongExtra(
+                MockIntents.EXTRA_DELAY_FOR_VIDEO_RECEPTION, DEFALUT);
+        final long delayForDataUsageCallback = intent.getLongExtra(
+                MockIntents.EXTRA_DELAY_FOR_DATA_USAGE_CALLBACK, DEFALUT);
+        for (MockConfigurationListener listener : mConfigurationListeners) {
+            listener.onConfigurationApplied(peerVideoEnabled, peerCvoSupport,
+                    delayForVideoReception, delayForDataUsageCallback);
+        }
+    }
+}
