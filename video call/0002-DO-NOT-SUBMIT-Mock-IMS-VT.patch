+
+    private static final long DEFAULT_UPLINK_USAGE_AUGEND = 500L; // byte.
+    private static final long DEFAULT_DOWNLINK_USAGE_AUGEND = 500L; // byte.
+    static final long DEFAULT_MEDIA_DELAY = 1000L; // msec.
+    static final long DEFAULT_USAGE_DELAY = 50L; // msec.
+
+    private static MockImsMedia sInstance;
+    private Surface mFarendSurface;
+    private MediaPlayer mMediaPlayer;
+    private Context mContext;
+    private boolean mUseMockImages;
+    private long mDelayToPlayMedia;
+    private long mDelayToSendDataUsage;
+    private boolean mPlayerRunning;
+    private long mUplink;
+    private long mDownlink;
+    private long mUplinkAugend = DEFAULT_UPLINK_USAGE_AUGEND;
+    private long mDownlinkAugend = DEFAULT_DOWNLINK_USAGE_AUGEND;
+
+    private final PhoneStateListener mPhoneStateListener = new PhoneStateListener() {
+        @Override
+        public void onPreciseCallStateChanged(PreciseCallState callState) {
+            handleCallStateChanged(callState);
+        }
+    };
+
+    private final Handler mHandler = new Handler() {
+        @Override
+        public void handleMessage(Message msg) {
+            if (msg.what == EVENT_DELAYED_MEDIA_PLAY) {
+                handleDelayedPlayMedia();
+            } else if (msg.what == EVENT_SEND_DATA_USAGE) {
+                final int mediaId = ((Integer) msg.obj).intValue();
+                handleDataUsageSending(mediaId);
+            }
+        }
+    };
+
+    /**
+     * Gets MockImsMedia singleton.
+     */
+    public static synchronized MockImsMedia getInstance() {
+        if (sInstance == null) {
+            sInstance = new MockImsMedia();
+        }
+        return sInstance;
+    }
+
+    /**
+     * Constructor for MockImsMedia
+     */
+    protected MockImsMedia() {
+        super();
+        MockCommandHandler.getInstance().addConfigurationListener(this);
+
+    }
+
+    /**
+     * Initializes Mock Media
+     */
+    @Override
+    int init() {
+        Log.d(TAG, "init");
+        final int ret = super.init();
+
+        // Please set this property to false if you met a fatal problem related to Mock images.
+        // The next call session will not play the images if do that.
+        mUseMockImages = SystemProperties.getBoolean(CONFIG_KEY_USE_MOCK_IMAGES, true);
+
+        // We can change a length of time that starts to play Mock images by this property. (msec)
+        mDelayToPlayMedia = SystemProperties.getLong(CONFIG_KEY_MEDIA_DELAY, DEFAULT_MEDIA_DELAY);
+
+        // Used to send back a data usage when clients request to know. This property can be
+        // change if wants to adjust. (msec)
+        mDelayToSendDataUsage = SystemProperties
+                .getLong(CONFIG_KEY_USAGE_DELAY, DEFAULT_USAGE_DELAY);
+
+        TelephonyManager.from(mContext).listen(mPhoneStateListener,
+                PhoneStateListener.LISTEN_PRECISE_CALL_STATE);
+        MockCommandHandler.getInstance().addListener(this);
+
+        // Sets the CVO mode in accordance with configuration.
+        int uiOrientationMode;
+        if (SystemProperties.getBoolean(CONFIG_KEY_CVO_SUPPORT, true)) {
+            uiOrientationMode = ImsMediaConstants.DYNAMIC_MODE;
+        } else {
+            uiOrientationMode = ImsMediaConstants.PORTRAIT_MODE;
+        }
+        setField("mUIOrientationMode", uiOrientationMode);
+        return ret;
+    }
+
+    /**
+     * Deinitializes Mock Media
+     */
+    @Override
+    void deInit() {
+        Log.d(TAG, "deInit");
+        TelephonyManager.from(mContext).listen(mPhoneStateListener, PhoneStateListener.LISTEN_NONE);
+        MockCommandHandler.getInstance().removeListener(this);
+        releaseMedia();
+        mFarendSurface = null;
+        mPlayerRunning = false;
+        mUplink = 0;
+        mDownlink = 0;
+        setField("mPeerWidth", ImsMediaConstants.DEFAULT_WIDTH);
+        setField("mPeerHeight", ImsMediaConstants.DEFAULT_HEIGHT);
+        setField("mVideoQualityLevel", ImsMediaConstants.VIDEO_QUALITY_UNKNOWN);
+        mHandler.removeMessages(EVENT_DELAYED_MEDIA_PLAY);
+        mHandler.removeMessages(EVENT_SEND_DATA_USAGE);
+        super.deInit();
+    }
+
+    /**
+     * Sets the far-end surface to display the mock images into that.
+     */
+    @Override
+    public void setSurface(Surface st) {
+        Log.d(TAG, "setSurface surface : " + st);
+        super.setSurface(st);
+        mFarendSurface = st;
+        if (mFarendSurface == null) {
+            // There is no surface to draw playing media. So should release.
+            releaseMedia();
+        } else {
+            if (hasActiveVideoCall()) {
+                playMedia();
+            }
+        }
+    }
+
+    /**
+     * Requests Call Data Usage
+     */
+    @Override
+    public void requestCallDataUsage(final int mediaId) {
+        if (!mHandler.hasMessages(EVENT_SEND_DATA_USAGE)) {
+            Message message = mHandler.obtainMessage(EVENT_SEND_DATA_USAGE, mediaId);
+            mHandler.sendMessageDelayed(message, mDelayToSendDataUsage);
+        }
+    }
+
+    /**
+     * Handles the change of the far-end device's orientation.
+     */
+    @Override
+    public void onMockedPeerResolutionChanged(int width, int height) {
+        Log.d(TAG, "onMockedPeerResolutionChanged width : " + width + ", height : " + height);
+        setField("mPeerWidth", width);
+        setField("mPeerHeight", height);
+        if (!mPlayerRunning) {
+            // Since this means that the far-end is not sending images,
+            // should not notify of a change of resolution.
+            Log.d(TAG, "Ignore notifying of peer resolution");
+            return;
+        }
+        invoke("mMediaListener", "onPeerResolutionChanged", getPeerWidth(), getPeerHeight());
+    }
+
+    /**
+     * Handles the change of the player state.
+     */
+    @Override
+    public void onMockedPlayerStateChanged(int state) {
+        Log.d(TAG, "onMockedPlayerStateChanged state : " + state);
+        boolean playerRunning = (state == PLAYER_STATE_STARTED);
+        if (mPlayerRunning == playerRunning) {
+            Log.d(TAG, "Already running");
+            return;
+        }
+        mPlayerRunning = playerRunning;
+        if (mPlayerRunning) {
+            resumeMedia();
+        } else {
+            pauseMedia();
+        }
+        invoke("mMediaListener", "onPlayerStateChanged", state);
+    }
+
+    /**
+     * Handles the change of the video quality.
+     */
+    @Override
+    public void onMockedVideoQualityEvent(int videoQuality) {
+        Log.d(TAG, "onMockedVideoQualityEvent quality : " + videoQuality);
+        setField("mVideoQualityLevel", videoQuality);
+        invoke("mMediaListener", "onVideoQualityEvent", getVideoQualityLevel());
+    }
+
+    /**
+     * Handles the change of the data usage.
+     */
+    @Override
+    public void onMockedDataUsageChanged(int mediaId, long uplink, long downlink) {
+        Log.d(TAG, "onMockedDataUsageChanged media ID : " + mediaId + ", uplink : " + uplink
+                + ", downlink : " + downlink);
+        // Only save with given values. These will be used as the augend to send to clients
+        // when they request a usage.
+        mUplinkAugend = uplink;
+        mDownlinkAugend = downlink;
+    }
+
+    /**
+     * Handles the change of the configurations.
+     */
+    @Override
+    public void onConfigurationApplied(boolean peerVideoEnabled, boolean peerCvoSupport,
+            long delayForVideoReception, long delayForDataUsageCallback) {
+        Log.d(TAG, "onMockedConfigurationApplied peer video enabled : " + peerVideoEnabled
+                + ", peer CVO support : " + peerCvoSupport + ", delay for video reception : "
+                + delayForVideoReception + ", delay for data usage callback : "
+                + delayForDataUsageCallback);
+        SystemProperties.set(CONFIG_KEY_USE_MOCK_IMAGES, Boolean.toString(peerVideoEnabled));
+        SystemProperties.set(CONFIG_KEY_CVO_SUPPORT, Boolean.toString(peerCvoSupport));
+        if (delayForVideoReception >= 0) {
+            SystemProperties.set(CONFIG_KEY_MEDIA_DELAY, Long.toString(delayForVideoReception));
+        }
+        if (delayForDataUsageCallback >= 0) {
+            SystemProperties.set(CONFIG_KEY_USAGE_DELAY, Long.toString(delayForDataUsageCallback));
+        }
+    }
+
+    /**
+     *  {@inheritDoc}
+     */
+    @Override
+    public boolean onInfo(MediaPlayer mp, int what, int extra) {
+        boolean handled = false;
+        if (what == MediaPlayer.MEDIA_INFO_VIDEO_RENDERING_START) {
+            // Simulates that the video reception has resumed.
+            onMockedPlayerStateChanged(PLAYER_STATE_STARTED);
+            // Peer resolution needs to be notified to client when starts a player.
+            onMockedPeerResolutionChanged(getPeerWidth(), getPeerHeight());
+            handled = true;
+        }
+        return handled;
+    }
+
+    /**
+     * Sets Context instance to MockImsMedia.
+     */
+    void setContext(Context context) {
+        mContext = context;
+    }
+
+    /**
+     * Handles a precise call state.
+     */
+    private void handleCallStateChanged(PreciseCallState callState) {
+        if (hasActiveVideoCall()) {
+            playMedia();
+        } else {
+            releaseMedia();
+        }
+    }
+
+    /**
+     * Handles delayed media play.
+     */
+    private void handleDelayedPlayMedia() {
+        if (mMediaPlayer != null && !mMediaPlayer.isPlaying()) {
+            mMediaPlayer.start();
+        }
+    }
+
+    /**
+     * Handles data usage sending.
+     */
+    private void handleDataUsageSending(int mediaId) {
+        mUplink += mUplinkAugend;
+        mDownlink += mDownlinkAugend;
+        // Uplink and downlink will be unified as long, so the total value must
+        // not be greater than its max.
+        if (mUplink > (Long.MAX_VALUE / 2)) {
+            mUplink = (Long.MAX_VALUE / 2);
+        }
+        if (mDownlink > (Long.MAX_VALUE / 2)) {
+            mDownlink = (Long.MAX_VALUE / 2);
+        }
+        Log.d(TAG, "requestCallDataUsage media ID : " + mediaId + ", uplink : " + mUplink
+                + ", downlink : " + mDownlink);
+        invoke("mMediaListener", "onDataUsageChanged", mediaId, mUplink, mDownlink);
+    }
+
+    /**
+     * Returns an active call type.
+     */
+    private int getActiveCallType() {
+        int type = CallDetails.CALL_TYPE_UNKNOWN;
+        final ImsCallSessionImpl session = ImsVideoGlobals.getInstance().getActiveCallSession();
+        if (session != null) {
+            type = session.getInternalCallType();
+        }
+        return type;
+    }
+
+    /**
+     * Checks whether or not there is an active video call.
+     */
+    private boolean hasActiveVideoCall() {
+        // Since this is used to confirm whether or not the far-end video should
+        // be shown, we have to treat as video type only if contains rx.
+        final int callType = getActiveCallType();
+        return callType == CallDetails.CALL_TYPE_VT
+                || callType == CallDetails.CALL_TYPE_VT_RX
+                || callType == CallDetails.CALL_TYPE_VT_RESUME;
+    }
+
+    /**
+     * Plays a media (Mock images) with the far-end surface.
+     */
+    private void playMedia() {
+        if (!mUseMockImages || mFarendSurface == null || mMediaPlayer != null) {
+            return;
+        }
+        final String externalMediaPath = getExternalMediaPath();
+        if (TextUtils.isEmpty(externalMediaPath)) {
+            // Use default images.
+            mMediaPlayer = MediaPlayer.create(mContext, R.raw.mock_images_default);
+        } else {
+            // Create a player with an external media. We must hold the created
+            // instance only when preparing is successfully completed.
+            MediaPlayer player = new MediaPlayer();
+            try {
+                player.setDataSource(externalMediaPath);
+                player.prepare();
+                mMediaPlayer = player;
+            } catch (IOException e) {
+                // Nothing to do.
+            }
+        }
+        if (mMediaPlayer == null) {
+            Log.e(TAG, "Cannot create media player");
+            return;
+        }
+        Log.d(TAG, "Play media");
+        mMediaPlayer.setVideoScalingMode(MediaPlayer.VIDEO_SCALING_MODE_SCALE_TO_FIT_WITH_CROPPING);
+        mMediaPlayer.setLooping(true);
+        mMediaPlayer.setOnInfoListener(this);
+        mMediaPlayer.setSurface(mFarendSurface);
+        if (mDelayToPlayMedia > 0) {
+            mHandler.removeMessages(EVENT_DELAYED_MEDIA_PLAY);
+            mHandler.sendEmptyMessageDelayed(EVENT_DELAYED_MEDIA_PLAY, mDelayToPlayMedia);
+        } else {
+            mMediaPlayer.start();
+        }
+    }
+
+    /**
+     * Releases a media.
+     */
+    private void releaseMedia() {
+        if (mMediaPlayer == null) {
+            return;
+        }
+        Log.d(TAG, "Release media");
+        mMediaPlayer.stop();
+        mMediaPlayer.release();
+        mMediaPlayer = null;
+    }
+
+    /**
+     * Pauses a media.
+     */
+    private void pauseMedia() {
+        if (mMediaPlayer != null && mMediaPlayer.isPlaying()) {
+            Log.d(TAG, "Pause media ");
+            mMediaPlayer.pause();
+        }
+    }
+
+    /**
+     * Resumes a media.
+     */
+    private void resumeMedia() {
+        if (mMediaPlayer != null && !mMediaPlayer.isPlaying()) {
+            Log.d(TAG, "Resume media ");
+            mMediaPlayer.start();
+        }
+    }
+
+    /**
+     * Gets an external media path.
+     * This returns a media path only when it exists.
+     */
+    private String getExternalMediaPath() {
+        String path = Environment.getExternalStorageDirectory().getPath() + EXTERNAL_MEDIA_PATH;
+        Log.d(TAG, "External media path : " + path);
+        File file = new File(path);
+        if (file.exists()) {
+            return path;
+        }
+        return null;
+    }
+
+    /**
+     * Sets a value to specified field that is declared in super class as private.
+     */
+    private void setField(String fieldName, Object value) {
+        Field f = null;
+        try {
+            f = getClass().getSuperclass().getDeclaredField(fieldName);
+            f.setAccessible(true);
+            f.set(this, value);
+        } catch (Exception e) {
+            Log.e(TAG, "setField : " + e.getMessage());
+        } finally {
+            if (f != null) f.setAccessible(false);
+        }
+    }
+
+    /**
+     * Invokes a private method that is declared in super class or private fields.
+     * An argument fieldName can be null to invoke super method.
+     */
+    private Object invoke(String fieldName, String methodName, Object... arguments) {
+        Object ret = null;
+        Field f = null;
+        Method m = null;
+        Object receiver = null;
+        Class<?>[] argumentTypes = getArgumentTypes(arguments);
+        try {
+            if (fieldName != null) {
+                f = getClass().getSuperclass().getDeclaredField(fieldName);
+                f.setAccessible(true);
+                receiver = f.get(this);
+                m = receiver.getClass().getDeclaredMethod(methodName, argumentTypes);
+            } else {
+                receiver = this;
+                m = getClass().getSuperclass().getDeclaredMethod(methodName, argumentTypes);
+            }
+            m.setAccessible(true);
+            ret = m.invoke(receiver, arguments);
+        } catch (Exception e) {
+            Log.e(TAG, "invoke : " + e.getMessage());
+        } finally {
+            if (f != null) f.setAccessible(false);
+            if (m != null) m.setAccessible(false);
+        }
+        return ret;
+    }
+
+    /**
+     * Converts arguments to type list of arguments.
+     */
+    private Class<?>[] getArgumentTypes(Object... arguments) {
+        Class<?>[] argumentTypes = null;
+        if (arguments != null) {
+            argumentTypes = new Class[arguments.length];
+            // Assumes that there is no need to use class type wrappers. Since invoke() takes
+            // arguments as Object to provide generic usage, primitive value will be cast to class
+            // type wrapper such as Integer Boolean and so on. However getDeclaredMethod()
+            // needs primitive if wanted method is declared as taking primitive arguments.
+            for (int i = 0; i < arguments.length; i++) {
+                Object argument = arguments[i];
+                if (argument instanceof Boolean) {
+                    argumentTypes[i] = boolean.class;
+                } else if (argument instanceof Character) {
+                    argumentTypes[i] = char.class;
+                } else if (argument instanceof Byte) {
+                    argumentTypes[i] = byte.class;
+                } else if (argument instanceof Short) {
+                    argumentTypes[i] = short.class;
+                } else if (argument instanceof Integer) {
+                    argumentTypes[i] = int.class;
+                } else if (argument instanceof Long) {
+                    argumentTypes[i] = long.class;
+                } else if (argument instanceof Float) {
+                    argumentTypes[i] = float.class;
+                } else if (argument instanceof Double) {
+                    argumentTypes[i] = double.class;
+                } else {
+                    // Usual class type should be taken as is. Also Void wrapper too.
+                    argumentTypes[i] = argument.getClass();
+                }
+            }
+        }
+        return argumentTypes;
+    }
+}
