+class MockImsMedia extends ImsMedia implements MockCommandHandler.MockListener,
+        MockCommandHandler.MockConfigurationListener, MediaPlayer.OnInfoListener {
+
+    private static final String TAG = "VideoCall_MockImsMedia";
+
+    private static final int EVENT_DELAYED_MEDIA_PLAY = 0;
